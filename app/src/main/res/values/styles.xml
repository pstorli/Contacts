<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/ColorPrimary</item>
        <item name="colorPrimaryDark">@color/ColorPrimaryDark</item>
        <item name="colorAccent">@color/ColorAccent</item>

        <item name="dialogHeaderBackground">@color/blue</item>
        <item name="dialogTitleColor">@color/black</item>
        <item name="dialogTextColor">@color/black</item>
        <item name="dialogHeaderIcon">@mipmap/ic_info</item>
        <item name="dialogBtnTextColor">@color/black</item>

        <item name="dialogNegBtnText">@string/no</item>
        <item name="dialogNeuBtnText">@string/cancel</item>
        <item name="dialogPosBtnText">@string/ok</item>
    </style>

</resources>
